{
  "name": "react-snack",
  "version": "0.0.1",
  "description": "Snacks, lots of them",
  "author": "juliankrispel",
  "license": "MIT",
  "repository": "juliankrispel/react-snack",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "flow": "flow",
    "apidocs": "documentation build src/index.js -f md -o docs/apidocs.md",
    "lint": "eslint src",
    "prettier": "prettier src/**/*.js --write",
    "precommit": "lint-staged && yarn run lint && yarn run flow && yarn run test",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "documentation": "^8.0.0",
    "rollup-plugin-babel-minify": "^5.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "uuid": "^3.2.1"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^^16.3.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.0.0",
    "styled-components": "^3.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-flowtype": "^2.36.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.74.0",
    "gh-pages": "^1.1.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-scripts": "^1.1.1",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "rollup-plugin-postcss": "^1.1.0",
    "rollup-plugin-url": "^1.3.0"
  },
  "files": [
    "dist"
  ]
}
